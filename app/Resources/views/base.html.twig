<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}LAN des SIO{% endblock %}</title>
        <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}" />
        <link rel="icon" type="image/x-icon" href="{{ asset('img/favicon.ico') }}"/>
        <link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/sweetalert.css') }}" />
        <link rel="stylesheet" type="text/css" href="{{ asset('css/navbars.css') }}" />
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        <script type="text/javascript" src="{{ asset('js/jquery-3.1.1.min.js') }}"></script>
        <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
        <script type="text/javascript" src="{{ asset('js/bootstrap.min.js') }}"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
        <script type="text/javascript" src="{{ asset('js/sweetalert.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/alertSystem.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/game.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/invite.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/notify.js') }}"></script>
        {% block javascripts %}{% endblock %}
        <script type="text/javascript">
            {% block scripts %}{% endblock %}
        </script>
    </body>
</html>
